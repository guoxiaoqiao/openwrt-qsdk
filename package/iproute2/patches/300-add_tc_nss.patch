+		return 0;
+	if (RTA_PAYLOAD(opt) < sizeof(*qopt))
+		return -1;
+	qopt = RTA_DATA(opt);
+
+	if (qopt->r2q != 0)
+		fprintf(f, "r2q %x ", qopt->r2q);
+
+	return 0;
+}
